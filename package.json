{
	"name": "react-mui-exam",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"airbnb",
			"prettier",
			"prettier/react",
			"plugin:prettier/recommended",
			"eslint-config-prettier",
			"react-hooks"
		],
		"parser": "babel-eslint",
		"rules": {
			"react/global-require": "off",
			"react/import/prefer-default-export": "off",
			"react-hooks/rules-of-hooks": "error",
			"no-unused-vars": 1,
			"react-hooks/exhaustive-deps": [
				"warn",
				{
					"additionalHooks": "useRecoilCallback"
				}
			],
			"react/jsx-filename-extension": [
				1,
				{
					"extensions": [
						".js",
						".jsx"
					]
				}
			],
			"prettier/prettier": [
				2,
				{
					"trailingComma": "all",
					"useTabs": true,
					"tabWidth": 2,
					"semi": true,
					"singleQuote": false,
					"jsxSingleQuote": false,
					"printWidth": 160
				}
			]
		},
		"plugins": [
			"prettier"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@emotion/react": "^11.5.0",
		"@emotion/styled": "^11.3.0",
		"@mui/core": "^5.0.0-alpha.53",
		"@mui/icons-material": "^5.0.4",
		"@mui/material": "^5.0.4",
		"@mui/styled-engine-sc": "^5.0.3",
		"@mui/styles": "^5.0.2",
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"clsx": "^1.1.1",
		"cookie-parser": "^1.4.4",
		"cookies": "^0.8.0",
		"cors": "^2.8.5",
		"csstype": "^3.0.5",
		"express": "^4.17.1",
		"express-asyncify": "^1.0.1",
		"express-session": "^1.17.1",
		"global": "^4.4.0",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mime": "^2.4.7",
		"mobx": "^5.10.1",
		"mobx-react-lite": "^1.4.0",
		"mobx-state-tree": "^3.14.0",
		"moment": "^2.29.1",
		"next": "^12.0.2",
		"next-cookies": "^2.0.3",
		"numeral": "^2.0.6",
		"prop-types": "^15.7.2",
		"react": "17.0.2",
