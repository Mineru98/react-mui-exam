		"react-dom": "17.0.2",
		"react-spring": "^8.0.27",
		"react-use": "^17.3.1",
		"react-window": "^1.8.6",
		"styled-components": "^5.3.3",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-decorators": "^7.12.1",
